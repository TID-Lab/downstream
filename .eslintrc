{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    },
    "extends": [
      "airbnb-typescript"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "no-restricted-syntax": "off"
    },
    "ignorePatterns": [ "builtin.js" ]
  }